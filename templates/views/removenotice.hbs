<!DOCTYPE html>
<html lang="en">
{{>headers}}
<body>
    {{>adminnavbar}}
     <h3 class="trade-header" id="trade-header">Books</h3>
     <hr>
      <form action="/adminnotice/removent" method="POST" class="deletebooks">
     <div class="input-group">
  <div class="form-outline">
    <input type="search" id="form1" class="form-control" name="title"/>
    <label class="form-label" for="form1">Notice Title</label>
  </div>
  <button type="submit" class="btn btn-danger">
    <i class="fas fa-trash-can"></i>
  </button>

</div>
</form>
    <div class="root-container card-container normal books" id="root-container">

    {{#each notice}}
     <div class="card ptag" style="width: 18rem;">
  <img src="../img/img6.jpg" class="card-img-top" alt="Sunset Over the Sea"/>
  <div class="card-body ptag">
      <h2 class="book-title">{{this.title}}</h2>
      <p>{{this.subject}}</p>
      
  </div>
</div>
     {{/each}}
    </div>

  <script src="../js/main.js"></script>
  <script
  type="text/javascript"
  src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/4.0.0/mdb.min.js"
></script>
</body>
</html>